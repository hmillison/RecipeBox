
<br>
<ul>
<% if(user != "empty") {%>
<% for(var i=0; i<user.recipes.length; i++) {%>
<% if(i % 4 == 0) {%>
	<div class="row">
<% } %>
<div class="col-sm-6 col-lg-3" id="<%= i %>"> 
<div class="thumbnail" >
	<img src="<%= user.recipes[i].image %> " alt="Recipe Details" class="foodpic" id="<%= i %>">
	<h3><%= user.recipes[i].name %></h3>
		<div class="pagination-centered">
			<button class="btn btn-primary" id="listadd<%= i %>"role="button">Add to List</button> 
	 			<button class="btn btn-default" id="delete<%= i %>" role="button">Delete</button>
	 	</div> 
		
</div>
</div>
<% if((i + 1) % 4 == 0)  {%>
	</div>
<% } } } %>
</ul>

